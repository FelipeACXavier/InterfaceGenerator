{
  "name": "BouncingBall",
  "description": "A bouncing ball model",
  "authors": "Author's name",
  "version": "1.0.0",
  "formalism": "continuous",
  "server": "matlab2024a",
  "client": "openrti1516",
  "step": 1000,
  "minimumstep": 100,
  "lookahead": 5000,
  "superdensetime": false,
  "inputs": [
    {
      "id": 0,
      "name": "h0",
      "description": "Initial height of the ball",
      "type": "float64",
      "unit": "m",
      "default": 5
    },
    {
      "id": 1,
      "name": "v0",
      "description": "Initial velocity of the ball",
      "type": "float64",
      "unit": "m/s",
      "default": 0
    }
  ],
  "outputs": [
    {
      "id": 0,
      "name": "h",
      "description": "Height of the ball",
      "type": "float64",
      "unit": "m"
    },
    {
      "id": 1,
      "name": "v",
      "description": "Velocity of the ball",
      "type": "float64",
      "unit": "m/s"
    }
  ],
  "parameters": [
    {
      "id": 0,
      "name": "g",
      "description": "Gravity",
      "type": "float64",
      "unit": "m/s^2",
      "default": 9.81
    },
    {
      "id": 1,
      "name": "e",
      "description": "Coefficient of restitution",
      "type": "float64",
      "default": 0.7
    }
  ]
}