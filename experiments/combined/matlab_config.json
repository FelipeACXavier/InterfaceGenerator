  "outputs": [
    {
      "id": 0,
      "name": "force",
      "description": "Resulting force",
      "type": "float64",
      "unit": "N.m",
      "namespace": "InteractionRoot"
    }
  ],
  "parameters": [
    {
      "id": 0,
      "name": "floor",
      "description": "Floor height",
      "type": "float64",
      "unit": "m",
      "default": 0.0,
      "namespace": "InteractionRoot.Parameters"
    },
    {
      "id": 1,
      "name": "mass",
      "description": "Mass of the object",
      "type": "float64",
      "unit": "kg",
      "default": 50,
      "namespace": "InteractionRoot.Parameters"
    },
    {
      "id": 2,
      "name": "v0",
      "description": "Initial velocity",
      "type": "float64",
      "unit": "m/s",
      "default": 0.0,
      "namespace": "InteractionRoot.Parameters"
    }
  ]
